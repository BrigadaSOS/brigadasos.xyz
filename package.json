{
  "name": "brigada-sos",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "docusaurus start",
    "build": "docusaurus build",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "prepare": "husky",
    "lint": "npm run lint:eslint && npm run lint:markdown && npm run lint:spell",
    "lint:eslint": "eslint . --max-warnings 0",
    "lint:markdown": "markdownlint-cli2 \"**/*.md\"",
    "lint:spell": "cspell \"**/*.{md,js,jsx,ts,tsx,json}\"",
    "format": "npm run format:prettier && npm run format:markdown",
    "format:prettier": "prettier --write .",
    "format:markdown": "markdownlint-cli2 --fix \"**/*.md\"",
    "format:check": "prettier --check ."
  },
  "dependencies": {
    "@docusaurus/core": "^3.9.1",
    "@docusaurus/preset-classic": "^3.9.1",
    "@easyops-cn/docusaurus-search-local": "^0.52.1",
    "@mdx-js/react": "^3.0.0",
    "clsx": "^2.0.0",
    "prism-react-renderer": "^2.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-player": "^2.10.1"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@docusaurus/eslint-plugin": "^3.9.1",
    "@eslint/js": "^9.37.0",
    "@typescript-eslint/eslint-plugin": "^8.46.0",
    "@typescript-eslint/parser": "^8.46.0",
    "cspell": "^9.2.1",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.3",
    "markdownlint-cli2": "^0.18.1",
    "prettier": "^3.6.2"
  }
}
